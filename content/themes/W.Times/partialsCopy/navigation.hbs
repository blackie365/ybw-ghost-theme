{{#foreach navigation}}
<li class="{{slug}} {{#if current}}current-menu-item{{/if}}"><a href="{{url absolute="true"}}">{{label}}</a></li>
{{/foreach}}
{{#if @site.members_enabled}}
{{#if @member}}
<li><a class="menu-btn" href="/account/">{{t "Account"}}</a></li>
<li><a href="#signout" data-members-signout>{{t "Sign out"}}</a></li>
{{else}}

<li><a class="menu-btn" href="/signup/">{{t "Sign up"}}</a></li>
{{/if}}
{{/if}}